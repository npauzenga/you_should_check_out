<div class="container">
  <div class="row">
    <div class ="resource">
        <% @resources.each do |resource| %>
        <h1><a class="title" href="<%= resource.link %>"><%= resource.title %></a></h1>
        <p class="description"><%= resource.description %></p>
        <% end %>
    </div>
  </div>
</div>

  
  